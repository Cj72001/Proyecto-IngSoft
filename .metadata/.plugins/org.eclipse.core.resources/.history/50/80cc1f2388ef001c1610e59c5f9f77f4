


create table `CINE` (
	`ID_CINE` int (11),
	`NOMBRE` varchar (90),
	`ASIENTOS` int (999),
	`SALAS` int (50),
	`TRABAJADORES` int (999)
); 

create table `PAISLOCALIDAD` (
	`ID_PAISLOCALIDAD` int (11),
	`NOMBRE` varchar (300),
	`CIUDAD` varchar (300),
	`CENTROCOMERCIAL` varchar (300),
	`ID_CINE` int (11)
); 



insert into `CINE` (`ID_CINE`, `NOMBRE`, `ASIENTOS`, `SALAS`, `TRABAJADORES`) values('1','cine mark', '200', '2', '90');
